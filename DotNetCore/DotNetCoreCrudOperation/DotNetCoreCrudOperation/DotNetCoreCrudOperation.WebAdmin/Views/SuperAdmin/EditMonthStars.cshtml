                    <div class="tab_content" id="tab_1">

                        <div class="form_fields_box">
                            <fieldset>
                                <div class="form_field">
                                    <label for="Stars_ID">Id* </label>
                                    <input type="text" disabled placeholder="#@Model.Id">
                                    <input type="hidden" asp-for="Id" />
                                </div>
                                <div class="form_field">
                                    <label for="Latest_Stars">
                                        Month*
                                        <small class="info_icon" data-toggle="tooltip" data-placement="right" title="(Required) Please enter an identifying name for this set of stars.">Tooltip</small>
                                    </label>
                                    <input type="text" asp-for="NewMonthName" />
                                    <span class="error_label" id="spanError_NewMonthName" asp-validation-for="NewMonthName"></span>

                                </div>
                                <div class="form_field">
                                    <div class="check_field">
                                        <input type="checkbox" asp-for="IsActive">
                                        <label for="Latest_Stars_IsActive?">Publish the month stars<small class="info_icon" data-toggle="tooltip" data-placement="right" title="" data-original-title="(optional) Check this to publish the month stars.">Tooltip</small></label>
                                    </div>
                                </div>
                                @*<div class="form_field">
                                        <label for="Stars_Month">
                                            Month*
                                            <small class="info_icon" data-toggle="tooltip" data-placement="right" title="(Required) Enter the month and year for this set of stars.">Tooltip</small>
                                        </label>
                                        <input type="text" asp-for="MonthYear" class="datepicker-here" data-language='en' data-min-view="months" data-view="months" readonly data-date-format="MM yyyy" />
                                        <span class="error_label" asp-validation-for="MonthYear"></span>
                                    </div>
                                    <input type="hidden" asp-for="MonthName" />
                                    <input type="hidden" asp-for="MonthNumber" />
                                    <input type="hidden" asp-for="Year" />*@
                            </fieldset>

                        </div>
                        <!--End form_fields_box-->

                    </div>

                    <div class="tab_content" id="tab_2">

                        <div class="form_fields_box">
                            <fieldset>
                                <input type="hidden" asp-for="Star1.Id" />
                                <div class="form_field">
                                    <label for="Star1_Name">
                                        Name
                                        <small class="info_icon" data-toggle="tooltip" data-placement="right" title="Enter the name of the star.">Tooltip</small>
                                    </label>
                                    <input asp-for="Star1.StarName" type="text">
                                    <span class="error_label" asp-validation-for="Star1.StarName"></span>
                                </div>

                                <div class="form_field">
                                    <label for="Star1_Role">
                                        Role
                                        <small class="info_icon" data-toggle="tooltip" data-placement="right" title="Enter the role of the star.">Tooltip</small>
                                    </label>
                                    <input asp-for="Star1.StarRole" type="text">
                                    <span class="error_label" asp-validation-for="Star1.StarRole"></span>
                                </div>

                                <div class="form_field">
                                    <label for="Star1_Location">
                                        Location
                                        <small class="info_icon" data-toggle="tooltip" data-placement="right" title="Enter the location of the star.">Tooltip</small>
                                    </label>
                                    <input asp-for="Star1.StarLocation" type="text">
                                    <span class="error_label" asp-validation-for="Star1.StarLocation"></span>
                                </div>

                                <div class="form_field">
                                    <label for="Star1_Photo">Photo <small class="info_icon" data-toggle="tooltip" data-placement="right" title="Select the photo of the star for this month (40 * 40 pixels).">Tooltip</small></label>
                                    <div class="image_upload right_upload">
                                        <span>Upload an image</span>
                                        <input type="file" class="upload" asp-for="Star1.StarPhotoData" id="fileUploaderImg1" />
                                        @if (Model.Star1.StarPhoto == null)
                                        {
                                            <img id="img1" />
                                            <div class="upload_remove" title="Remove Selected Star Image" id="remove_fileUploaderImg1_img1" style="display:none;"></div>
                                        }
                                        else
                                        {
                                            <img src="@Url.Content("~/fileserver/Uploads/Admin/Stars/" + Model.Star1.StarPhoto)" id="img1" />
                                            <div class="upload_remove" title="Remove Selected Star Image" id="remove_fileUploaderImg1_img1"></div>
                                        }
                                        <input type="hidden" asp-for="Star1.FrontEndUserImageForStar" class="hiddenFrontEndUserImageForStar" />
                                        <button type="button" class="OpenImagesPopUpForStar">Choose Pic</button>
                                        <input type="hidden" class="hiddenStarPhotoName" asp-for="Star1.StarPhoto" />
                                    </div>
                                </div>


                            </fieldset>

                        </div>
                        <!--End form_fields_box-->

                        <div class="form_fields_box">
                            <fieldset>
                                <div class="form_field">
                                    <label for="Star1_Value">Icon <small class="info_icon" data-toggle="tooltip" data-placement="right" title="Select the  value for this star.">Tooltip</small></label>
                                    <div class="select_div">
                                        <select asp-for="Star1.StarValueType">
                                            <option>Select...</option>
                                            <option>Communication</option>
                                            <option>Dedication</option>
                                            <option>Care</option>
                                            <option>Excellence</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="form_field">
                                    <label for="Star1_testtimonial">Testimonial <small class="info_icon" data-toggle="tooltip" data-placement="right" title="Enter the testimonial of the star.">Tooltip</small></label>
                                    <textarea asp-for="Star1.StarTestimonial"></textarea>
                                    <span class="error_label" asp-validation-for="Star1.StarTestimonial"></span>
                                </div>
                            </fieldset>
                        </div>

                    </div>

