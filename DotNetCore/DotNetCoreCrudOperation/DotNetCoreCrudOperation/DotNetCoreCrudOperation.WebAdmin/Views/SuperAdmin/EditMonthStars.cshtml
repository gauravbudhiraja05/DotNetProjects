<form id="formMonthStars" name="formFeaturedMessage">
    <div class="tab_menu">
        <div class="center_wrap">

            <button type="button" class="back_btn" onClick="window.location.href='@Url.Action("MonthStars","SuperAdmin")'">Back</button>

            <ul class="tab_list">
                <li class="current"><a id="tab1" href="#tab_1">Stars Attributes</a></li>
                <li><a href="#tab_2">Star 1</a></li>
                <li><a href="#tab_3">Star 2</a></li>
